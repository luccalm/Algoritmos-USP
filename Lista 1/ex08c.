#include <stdio.h>
#include <stdlib.h>
#include <math.h>

*/ 8.  Dado um inteiro não-negativo n, determinar n!  */

int main(){
	int n, soma = 0;

	printf("Digite n:");
	scanf("%d", &n);

	while(n > 0)
	{
		if(n % 2 == 0)
		{
			soma = soma + n;
		}
		n--;
	}

	printf("A soma dos pares é: [%d]", soma);

return 0;
}